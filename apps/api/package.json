{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/main.ts\" -e .ts",
    "dev:old": "ts-node-dev --transpile-only --no-notify ./src/main.ts",
    "start": "node ./dist/main.js",
    "build": "tsc",
    "lint": "tsc --noEmit && TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "db:seed": "yarn prisma db seed",
    "db:generate": "yarn prisma generate",
    "db:push": "yarn prisma db push"
  },
  "devDependencies": {
    "@boost/module": "^4.0.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/schema-ast": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.10",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "scripts": "*",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "typescript": "^5.0.2"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-prisma": "^3.47.0",
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "jsonwebtoken": "^9.0.0",
    "logger": "*",
    "prisma": "^4.11.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node-dev src/prisma/seed.ts"
  }
}
